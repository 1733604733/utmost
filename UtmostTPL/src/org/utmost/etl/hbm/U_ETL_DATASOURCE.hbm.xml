<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="U_ETL_DATASOURCE" table="U_ETL_DATASOURCE">
		<id name="uuid" column="UUID" length="36" type="java.lang.String">
			<generator class="uuid" />
		</id>
		<property name="drivername" type="java.lang.String">
			<column length="128" name="DRIVERNAME">
				<comment>名称</comment>
			</column>
		</property>
		<property name="classname" type="java.lang.String">
			<column length="128" name="CLASSNAME">
				<comment>CLASSNAME</comment>
			</column>
		</property>
		<property name="url" type="java.lang.String">
			<column length="128" name="URL">
				<comment>URL</comment>
			</column>
		</property>
		<property name="username" type="java.lang.String">
			<column length="128" name="USERNAME">
				<comment>用户名</comment>
			</column>
		</property>
		<property name="password" type="java.lang.String">
			<column length="128" name="PASSWORD">
				<comment>用户密码</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
